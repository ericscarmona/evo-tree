{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "evo-tree-api",
  "projects": {
    "evo-tree-api": {
      "root": "apps/evo-tree-api",
      "sourceRoot": "apps/evo-tree-api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/evo-tree-api",
            "main": "apps/evo-tree-api/src/main.ts",
            "tsConfig": "apps/evo-tree-api/tsconfig.app.json",
            "assets": ["apps/evo-tree-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/evo-tree-api/src/environments/environment.ts",
                  "with": "apps/evo-tree-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "evo-tree-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/evo-tree-api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/evo-tree-api"],
          "options": {
            "jestConfig": "apps/evo-tree-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "evo-tree": {
      "projectType": "application",
      "root": "apps/evo-tree",
      "sourceRoot": "apps/evo-tree/src",
      "prefix": "evo-tree",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/evo-tree",
            "index": "apps/evo-tree/src/index.html",
            "main": "apps/evo-tree/src/main.ts",
            "polyfills": "apps/evo-tree/src/polyfills.ts",
            "tsConfig": "apps/evo-tree/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/evo-tree/src/favicon.ico",
              "apps/evo-tree/src/assets"
            ],
            "styles": ["apps/evo-tree/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/evo-tree/src/environments/environment.ts",
                  "with": "apps/evo-tree/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "evo-tree:build:production"
            },
            "development": {
              "browserTarget": "evo-tree:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/evo-tree/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "evo-tree:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/evo-tree/src/**/*.ts",
              "apps/evo-tree/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/evo-tree"],
          "options": {
            "jestConfig": "apps/evo-tree/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "evo-tree-e2e": {
      "root": "apps/evo-tree-e2e",
      "sourceRoot": "apps/evo-tree-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/evo-tree-e2e/cypress.json",
            "tsConfig": "apps/evo-tree-e2e/tsconfig.e2e.json",
            "devServerTarget": "evo-tree:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "evo-tree:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/evo-tree-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "home-page-feature": {
      "root": "libs/home-page-feature",
      "sourceRoot": "libs/home-page-feature/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/home-page-feature/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/home-page-feature"],
          "options": {
            "jestConfig": "libs/home-page-feature/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "species-data-access": {
      "root": "libs/species-data-access",
      "sourceRoot": "libs/species-data-access/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/species-data-access/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/species-data-access"],
          "options": {
            "jestConfig": "libs/species-data-access/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "schematics": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
